<h1>Projects index</h1>

<% @projects.each do |p| %>
  <ul>
    <li>
      <h4><%= link_to(p.title, project_path(p.id)) %></h4>
      <p><%= p.description %></p>
      <p>Submitted by: <%= link_to p.user.email, user_path(p.user.id), :action => 'get', :method => 'get' %></p>
    </li>
  </ul>
<% end %>
